    "json-schema-ref-parser": "4.1.1",
    "lodash": "4.17.5",
    "node-uuid": ">= 1.4.1",
    "path": "^0.12.7",
    "postman-collection": ""
  },
  "author": "Tuhin Khare",
  "license": "Apache",
  "devDependencies": {
    "chai": "^4.1.2",
